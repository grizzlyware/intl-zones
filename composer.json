{
    "name": "grizzlyware/intl-zones",
    "description": "Provides a list of zones for countries",
    "keywords": [
        "grizzlyware",
        "intl",
        "zones",
        "states",
        "regions"
    ],
    "homepage": "https://github.com/grizzlyware/intl-zones",
    "license": "MIT",
    "authors": [
        {
            "name": "Grizzlyware Ltd",
            "email": "contact@grizzlyware.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.13"
    },
    "autoload": {
        "psr-4": {
            "Grizzlyware\\Zones\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Grizzlyware\\Zones\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {

            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
